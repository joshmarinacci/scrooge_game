            "type": "dialog",
            "dialog": [
                {
                    "person": "scrooge",
                    "text": "Oh, I can't bear this anymore. Take me away, gracious Bob.",
                    "key": "key_0.1225780711152673"
                },
                {
                    "person": "pheasant",
                    "text": "You called me gracious. Does that mean your heart is softening?",
                    "key": "key_0.473079722783577"
                },
                {
                    "person": "scrooge",
                    "text": "Nothing of the sort. I just don't like people badmouthing me.",
                    "key": "key_0.7538598388822104"
                },
                {
                    "person": "scrooge",
                    "text": "How about we see old Rob Ratchet. He's poor but never insults me.",
                    "key": "key_0.179443968046064"
                },
                {
                    "person": "pheasant",
                    "text": "As you wish.",
                    "key": "key_0.7358047145840472"
                }
            ],
            "visible": true,
            "next": null
        },
