    "actions": [
        {
            "id": "dialog_0.22591707026839924",
            "title": "talk to the voice",
            "itemid": "voice",
            "type": "dialog",
            "dialog": [
                {
                    "person": "user",
                    "text": "What? What happened? Where am I?",
                    "key": "key_0.7818247976283326"
                },
                {
                    "person": "narrator",
                    "text": "You have passed beyond the veil.",
                    "key": "key_0.7256016011299256"
                },
                {
                    "person": "narrator",
                    "text": "Open your eyes and walk towards the light.",
                    "key": "key_0.23234036987801487"
                },
                {
                    "person": "user",
                    "text": "How did I get here?",
                    "key": "key_0.3288680397112327"
                },
                {
                    "person": "narrator",
                    "text": "You have shuffled off your mortal coil. Please walk towards the light.",
                    "key": "key_0.23694040134774008"
                },
                {
                    "person": "user",
                    "text": "Oh, wait. I remember turning out the light and going to sleep. Is this a dream?",
                    "key": "key_0.8994660389140532"
                },
                {
                    "person": "narrator",
                    "text": "Dude, youâ€™re dead. DOA. Bereft of life. Climbed the stairwell to heaven.",
                    "key": "key_0.5705079638629333"
                },
                {
                    "person": "user",
                    "text": "So this isn't a dream?",
                    "key": "key_0.7294400783254777"
                },
                {
                    "person": "narrator",
                    "text": "You have rung down the curtain and joined the choir invisible!",
                    "key": "key_0.3905438592981667"
                },
                {
                    "person": "narrator",
                    "text": "Now please, walk towards the light. Your answers await.",
                    "key": "key_0.2899120576156109"
                },
                {
                    "person": "user",
                    "text": "Okay, okay. Jeez. You don't have to be crabby about it.",
                    "key": "key_0.2863521100222477"
                }
            ],
            "visible": false,
            "next": "script_0.8000648907251502"
        },
